<div class="entry">
  <!-- fields group -->
  {{#if fieldsGroup}}
    <i class="fa fa-indent fa-2x fa-pull-left"></i><p>{{{description}}}</p>
  <!-- other -->
  {{else}}
    {{#if parent}}
      <div class="row">
        <div class="documentation">
          <!-- property description -->
          <h2>{{name}} property</h2>
          <!-- characteristics -->
          <div class="list-group">
            <!-- changelog -->
            {{#if changelog.isNew}}
              <div class="list-group-item list-group-item-success"><span class="fa fa-info-circle fa-fw fa-2x"></span><strong>New property! </strong>{{{changelog.details}}}</div>
            {{/if}}
            {{#if changelog.isModified}}
              <div class="list-group-item list-group-item-warning"><strong>Modified property! </strong>{{{changelog.details}}}</div>
            {{/if}}
            {{#if changelog.isInfo}}
              <div class="list-group-item list-group-item-info"><i class="fa fa-info-circle fa-fw fa-2x"></i>{{{changelog.details}}}</div>
            {{/if}}
            {{#if required}}
              <p class="list-group-item"><i class="fa fa-asterisk fa-fw fa-2x"></i>&nbsp;Required</p>
            {{/if}}
            {{#if isArray}}
              <p class="list-group-item"><i class="fa fa-list-ul fa-fw fa-2x"></i>&nbsp;Array</p>
            {{/if}}
            {{#if closedChildren}}
              <p class="list-group-item"><i class="fa fa-cubes fa-fw fa-2x"></i>&nbsp;{{type}}</p>
            {{else}}
              {{#if children}}
                <p class="list-group-item"><i class="fa fa-cubes fa-fw fa-2x"></i>&nbsp;{{type}}</p>
              {{else}}
                <p class="list-group-item"><i class="fa fa-cube fa-fw fa-2x"></i>&nbsp;{{type}}</p>
              {{/if}}
            {{/if}}
            {{#if md}}
              <p class="list-group-item"><i class="fa fa-file-code-o fa-fw fa-2x"></i>&nbsp;Supports markdown ({{{md}}} syntax)</p>
            {{/if}}
            {{#if allowReference}}
              <p class="list-group-item"><i class="fa fa-code-fork fa-fw fa-2x"></i>&nbsp;Value can be a reference to a {{{type}}}</p>
            {{/if}}
            {{#if documentationUrl}}
              <p class="list-group-item"><i class="fa fa-book fa-fw fa-2x"></i><a href="{{documentationUrl}}" target="openapidocumentation">OpenAPI Specification</a></p>
            {{/if}}
          </div>
          <!-- description -->
          {{#if description}}
            <p>{{{description}}}</p>
            {{#if values}}
            <h2>Values</h2>
            <table id="table" class="table table-striped table-bordered table-condensed table-hover" >
              <tr>
                  <th>
                      Value
                  </th>
                  <th>
                      Default
                  </th>
                  <th>
                      Description
                  </th>
              </tr>

              {{#values}}
              <tr>
                <td>{{this.value}}</td>
                <td>{{this.default}}</td>
                <td>{{{this.description}}}</td>
              </tr>
              {{/values}}
            </table>
            {{/if}}
          {{/if}}
        </div>
      </div>
    {{/if}}
    <!-- type description -->
    {{#if typeDescription}}
    <div id="typedescription" class="row">
      <div class="documentation">
        <h2>{{type}}</h2>
        <div class="list-group">
        <!-- change log -->
        {{#if typeChangelog.isNew}}
          <div class="list-group-item list-group-item-success"><span class="fa fa-info-circle fa-fw fa-2x"></span><strong>New object! </strong>{{{typeChangelog.details}}}</div>
        {{/if}}
        {{#if typeChangelog.isModified}}
          <div class="list-group-item list-group-item-warning"><i class="fa fa-exclamation-triangle fa-fw fa-2x"></i>&nbsp;<strong>Modified object! </strong>{{{typeChangelog.details}}}</div>
        {{/if}}
        <!-- change log - deleted properties -->
        {{#if typeChangelog.deletedProperties}}
          <p class="list-group-item list-group-item-danger"><i class="fa fa-trash fa-fw fa-2x"></i>&nbsp;<a href="#deletedproperties">Deleted properties</a></p>
        {{/if}}
        <!-- change log - new properties -->
        {{#if typeChangelog.newProperties}}
          <p class="list-group-item list-group-item-success"><i class="fa fa-plus-square fa-fw fa-2x"></i>&nbsp;<a href="#newproperties">New properties</a></p>
        {{/if}}
        <!-- extension -->
        {{#if allowExtension}}
          <p class="list-group-item"><i class="fa fa-code-fork fa-fw fa-2x"></i>&nbsp;Allows extension with x- properties</p>
        {{/if}}
        <!-- documentation -->
        {{#if typeDocumentationUrl}}
          <p class="list-group-item"><i class="fa fa-book fa-fw fa-2x"></i>&nbsp;<a href="{{typeDocumentationUrl}}" target="openapidocumentation">OpenAPI Specification</a></p>
        {{/if}}
        </div>
        <!-- description -->
        {{#if typeDescription}}
        <h3>Description</h3>
        <p>{{{typeDescription}}}</p>
        {{/if}}
        <!-- new properties -->
        {{#if typeChangelog.newProperties}}
        <a name="newproperties"></a>
        <h3>New properties</h3>
        <table id="table" class="table table-striped table-bordered table-condensed" >
          <tr>
              <th>New Property</th>
              <th>Description</th>  
          </tr>
          {{#typeChangelog.newProperties}}
          <tr>
            <td>{{this.name}}</td>
            <td>{{{this.description}}}</td>
          </tr>
          {{/typeChangelog.newProperties}}
        </table>
        {{/if}}
        <!-- deleted properties -->
        {{#if typeChangelog.deletedProperties}}
        <a name="deletedproperties"></a>
        <h3>Deleted properties</h3>
        <table id="table" class="table table-striped table-bordered table-condensed" >
          <tr>
              <th>Deleted Property</th>
              <th>Replaced by</th>
              <th>Documentation</th>  
          </tr>
          {{#typeChangelog.deletedProperties}}
          <tr>
            <td>{{this.name}}</td>
            <td>{{this.replacedBy}}</td>
            <td><a href="" target="openapidocumentation"><i class="fa fa-book fa-fw fa-2x"></i></a></td>
          </tr>
          {{/typeChangelog.deletedProperties}}
        </table>
        {{/if}}
      </div>
    </div><!-- type description -->
    {{/if}}
  {{/if}}
</div>
